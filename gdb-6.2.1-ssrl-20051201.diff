+ static char **
+ split_cmdline (char *args, int *pnargs)
+ {
+   char  *blnk = args;
+   char **rval = 0;
+   int   nargs = 1; /* extra for arg[0] */
+ 
+   *pnargs = 0;
+   while (blnk)
+ 	{
+ 	  while ( isspace(*blnk) )
+ 		*blnk++ = 0;
+ 	  if ( *blnk )
+ 		{
+ 		  if ( !rval )
+ 			{
+ 			  *pnargs = nargs;
+ 			  blnk    = strdup(blnk);
+ 			  rval    = xcalloc(1,sizeof(*rval));
+ 			}
+ 		  nargs++;
+ 		  rval = xrealloc(rval, nargs*sizeof(*rval));
+ 		  rval[*pnargs] = blnk;
+ 		  *pnargs = nargs;
+ 		  blnk = strchr(blnk,' ');
+ 		}
+ 	  else
+ 		{
+ 		  blnk = 0;
+ 		}
+ 	}
+   return rval;
+ }
